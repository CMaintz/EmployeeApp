
@{
    ViewBag.Title = "Employee & Company Index";
}
@model WebGUI.ViewModel.HomeViewModel

<h2>@ViewBag.Title</h2>

<h2>Employees</h2>
@foreach (var employee in Model.Employees)
{
    @Html.Action("ShowEmployee", "Home", employee)
}

<h2>Companies</h2>
@foreach (var company in Model.Companies)
{
    @Html.Action("ShowCompany", "Home", company)
}